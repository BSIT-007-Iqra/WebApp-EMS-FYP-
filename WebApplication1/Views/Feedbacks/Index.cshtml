@model IEnumerable<WebApplication1.Models.Feedback>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layoutadmin.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Customer.Customer_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Event_tbl.Event_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Service.Service_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Venue.Venue_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Feedback_Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Feedback_Date)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Customer.Customer_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Event_tbl.Event_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Service.Service_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Venue.Venue_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Feedback_Description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Feedback_Date)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Feedback_ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.Feedback_ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Feedback_ID })
        </td>
    </tr>
}

</table>
