@using WebApplication1.Models;
@using WebApplication1.Utils;  
@{
    Model1 db = new Model1();

    int count = 0;
    List<int> list = new List<int>();

    DateTime startdate = DateTime.ParseExact("01/01/2022", "dd/MM/yyyy", null);
    DateTime enddate = DateTime.ParseExact("31/01/2022", "dd/MM/yyyy", null);
    for (int i = 0; i < 13; i++)
    {
        count = db.Bookings.Where(x => x.Event_Date >= startdate && x.Event_Date <= enddate && x.Booking_Type == "Sale").Count();
        if (count > 0)
        {
            //list.Add(Convert.ToInt32(db.Booking_Details.Where(x => x.Booking.Booking_Type== "Sale" && x.Booking.Booking_Status != "Cancelled" && x.Booking.Event_Date >= startdate && x.Booking.Event_Date <= enddate).Sum(x => (x.Quantity * -1) * x.Booking_Price)));
        }
        else
        {
            list.Add(0);
        }
        startdate = startdate.AddMonths(1);
        enddate = enddate.AddMonths(1);
        count = 0;

    }

}
