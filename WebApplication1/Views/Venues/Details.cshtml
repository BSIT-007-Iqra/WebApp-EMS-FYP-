<div class="row">
  <dl class="dl-horizontal">
            <dt>
                @Html.DisplayNameFor(model => model.Venue_Name)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Venue_Name)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.Venue_Details)
