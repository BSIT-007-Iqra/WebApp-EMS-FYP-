            </dt>

            <dd>
                @Html.DisplayFor(model => model.Venue_Location)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.Venue_Price)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Venue_Price)
            </dd>

        </dl>


    </div>
    <!-- Row --> @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <a href="@Url.Action("Edit", "Venues", new { id=Model.Venue_ID })">
                <i class="fa fa-edit btn btn-success  ">Edit</i>
            </a>
            <a class="btn btn-warning fa fa-arrow-circle-o-left" href="@Url.Action("Index","Venues")"> Back to List </a>
        </div>
    }
